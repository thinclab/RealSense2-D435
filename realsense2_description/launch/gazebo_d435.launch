<?xml version="1.0"?>
<launch>
  <node
    name="realsense_tf"
    pkg="tf"
    type="static_transform_publisher"
    args="2.2 0 1.6 0 0.349066 3.1415 root color_rgb_optical_frame 40" />
	<!-- 2.2 0 1.6 0 0.349066 3.1415 root kinect2_rgb_optical_frame 40 -->
  <param name="realsense_robot_description" command="$(find xacro)/xacro --inorder '$(find realsense2_description)/urdf/test_d435_camera.urdf.xacro'" />
  <node name="spawn_model" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen" args="-urdf -model realsense -param realsense_robot_description"/>
</launch>